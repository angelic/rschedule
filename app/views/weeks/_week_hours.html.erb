<% @dates.each do |date| %>
  <td>
    <% date.events.with_time_set.each do |event| %>
      <div>
        <dl>
          <dd>
            <%= event_time(event.start_time, event.end_time) %>
          </dd>
          <dt>
            <%= event_hour_desc(event) %>
          </dt>
        </dl>
      </div>
    <% end %>
  </td>
<% end %>